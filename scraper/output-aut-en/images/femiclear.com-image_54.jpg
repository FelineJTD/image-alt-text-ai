<script>

let lastScrollTop = 0;

document.addEventListener("scroll", () => {
  
  let scrollTop = window.pageYOffset || document.documentElement.scrollTop,
   header_sticky = document.getElementById('shopify-section-header'),
   announcement_sticky = document.getElementById('shopify-section-announcement-bar-custom').clientHeight,
   header_sticky_height = document.getElementById('shopify-section-header').clientHeight,
   totall_announ_header = announcement_sticky + header_sticky_height,
   progressBar_blog,
   validate_blog = false;

   if(document.querySelector('.template_article .progress-container')){
    progressBar_blog = document.querySelector('.progress-container');
    validate_blog = true;
   }

  if (scrollTop < lastScrollTop) {
    header_sticky.style.maxHeight = "100%";
    header_sticky.style.display = "block";

    if (validate_blog) {
      
      progressBar_blog.style.top = totall_announ_header + "px";
    }
    
  }else{
    header_sticky.style.maxHeight = "0px";
    header_sticky.style.display = "none";
    if (validate_blog) {
      progressBar_blog.style.top = announcement_sticky + "px";
    }
  }

  if (scrollTop <= 10) {
    header_sticky.style.maxHeight = "100%";
    header_sticky.style.display = "block";
    if (validate_blog) {     
      progressBar_blog.style.top = totall_announ_header + "px";
    }
  }

  lastScrollTop = scrollTop;
});

function change_price_ribbon() {

fetch('/cart.json',{
      method: 'GET',
      headers: {
        'Content-Type': 'application/json'
      }
    })
    .then(response => response.json())
    .then(data => {
     let cart = data;

      if (document.getElementById('price_shipping')){
        let price_change = document.getElementById('price_shipping'),
        current_price = (2499 - cart.items_subtotal_price) / 100,
        congrats_message = document.getElementById('congrats_message'),
        first_message = document.getElementById('message'),
        secong_message = document.getElementById('missing_text');


        price = current_price.toFixed(2);

        if(cart.items_subtotal_price >= 1 && cart.items_subtotal_price <= 2499) {
          price_change.innerHTML = "$" + price;
          price_change.style.display = "inline-block";
          secong_message.style.display = "block";
          congrats_message.style.display = "none";
          first_message.style.display = "none";
          console.log('no shipping', price) 
        }else if(cart.items_subtotal_price < 1){
          secong_message.style.display = "none";
          congrats_message.style.display = "none";
          first_message.style.display = "block";
        } else{
          first_message.style.display = "none";
          secong_message.style.display = "none";
          congrats_message.style.display = "block";
          console.log('shipping', current_price)
        }
      }
      
    }); 
}

$('.annoucement_bar__container.carrousel').slick({
