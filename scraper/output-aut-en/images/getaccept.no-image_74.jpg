      const ctaModalsBehavior = (formID) => {
        if (!document.querySelector(`#${formID}`)) return;
        
        const dialog = document.querySelector(`#${formID}`);
        
        const cta = document.querySelectorAll(`[data-modal="${formID}"]`);
        
        const close = document.querySelector(`#${formID} .modal-close`);
        
        const formContainer = document.querySelector(`#${formID} .form-container`);
        const form = document.querySelector(`#${formID} form`);

        const formInner = document.querySelector(`#${formID} .dialog-inner`);

        cta.forEach((el) =>
          el.addEventListener("click", () => {
            dialog.showModal();
            if (dialog.querySelector("video") && window.innerWidth > 1132) {
              dialog.querySelector("video").play();
            }
          })
        );
        close.addEventListener("click", () => {
          if (dialog.querySelector("video")) {
            dialog.querySelector("video").pause();
          }
          dialog.close();
        });
        dialog.addEventListener("click", (event) => {
          if (!formInner.contains(event.target)) {
            dialog.close();
          }
        });
      };
      
