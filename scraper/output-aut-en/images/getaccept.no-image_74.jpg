<script>
  setTimeout(() => {
    
    const hero = document.querySelector('[data-hero="true"]');
    if (!hero || hero.dataset.headerColorChange == "ignore") return;
    const header = document.querySelector("header");
    //We will use a intersection obeserver to check if the hero is in the viewport and if so set the header background to the hero background
    const observer = new IntersectionObserver(
      (entries) => {
        entries.forEach((entry) => {
          if (entry.isIntersecting) {
            header.setAttribute(
              "data-background",
              hero.getAttribute("data-background")
            );
          } else {
            header.setAttribute("data-background", "");
          }
        });
      },
      { threshold: [0, 1] }
    );
    observer.observe(hero);
  }, 200);
</script></div>
      
      

