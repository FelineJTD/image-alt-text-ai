    .lang-select .lang_switcher_class {
      display: flex;
      align-items: center;
    }

    .lang-select .lang_list_class {
      display: block;
      width: 0;
      height: 0;
      overflow: hidden;
      top: 30px;
      box-shadow: 0 8px 10px -2px rgba(0, 0, 0, 0.15),
